{
  "name": "cloud-feed-service",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "FarmIQ Cloud Feed Service - Feed master data and authoritative feed intake records",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "seed": "ts-node prisma/seed.ts",
    "start": "nyc ts-node src/index.ts",
    "build": "tsc && npm run copy-openapi",
    "copy-openapi": "cp openapi.yaml dist/",
    "serve": "node dist/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "eslint --ext .ts src/ --fix",
    "knip": "knip",
    "prettier": "prettier --write 'src/**/*.ts'",
    "prepare": "npx husky install",
    "premigrate": "tsc",
    "migrate:up": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate",
    "migrate:undo": "npx prisma migrate reset --force",
    "coverage": "npx nyc report --reporter=html && npx opener coverage/index.html",
    "generate-api-types": "openapi-typescript openapi.yaml -o src/api-types.ts"
  },
  "keywords": [],
  "author": "",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "@types/express": "^4.17.17",
    "@typescript-eslint/parser": "^6.21.0",
    "cors": "^2.8.5",
    "dd-trace": "^4.46.0",
    "eslint-plugin-typesafe": "^0.5.2",
    "express": "^4.21.0",
    "helmet": "^7.0.0",
    "js-yaml": "^4.1.0",
    "openapi-typescript": "^7.4.2",
    "swagger-ui-express": "^5.0.0",
    "ts-dotenv": "^0.9.1",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-react": "^7.22.5",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.16.5",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.8",
    "@types/amqplib": "^0.10.8",
    "@types/validator": "^13.12.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "dotenv-cli": "^7.2.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^46.4.6",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "knip": "^5.62.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "opener": "^1.5.2",
    "prisma": "^5.16.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}

