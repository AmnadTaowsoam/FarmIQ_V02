DATABASE_URL="postgresql://user:password@postgres-service:5432/dbname?schema=public"
DATADOG_RUM_ENV=local
COMMIT_ID=
NODE_ENV=development
APP_PORT=3000

# JWT Configuration (REQUIRED)
# MUST match the JWT_SECRET from cloud-identity-access
# Generate secure secrets using: openssl rand -base64 32
JWT_SECRET=your-jwt-secret-key-generate-with-openssl-rand-base64-32

# Downstream services
NOTIFICATION_SERVICE_URL="http://cloud-notification-service:3000"
NOTIFICATION_TIMEOUT_MS=2500
CLOUD_NOTIFICATION_SERVICE_URL="http://cloud-notification-service:3000"
