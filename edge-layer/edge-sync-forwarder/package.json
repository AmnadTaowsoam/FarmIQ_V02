{
  "name": "edge-sync-forwarder",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "FarmIQ Edge Sync Forwarder - outbox sync with claim/lease and DLQ",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "start": "nyc ts-node src/index.ts",
    "build": "tsc && npm run copy-openapi",
    "copy-openapi": "cp openapi.yaml dist/",
    "db:migrate": "tsx src/db/migrate.ts",
    "seed": "tsx src/seed.ts",
    "serve": "node dist/src/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "axios": "^1.7.9",
    "dd-trace": "^4.46.0",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "helmet": "^7.0.0",
    "js-yaml": "^4.1.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.13",
    "@types/js-yaml": "^4.0.9",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.8",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.16.5",
    "@types/supertest": "^2.0.16",
    "chai": "^4.5.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.16.1",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.4.5",
    "supertest": "^6.3.4"
  }
}
