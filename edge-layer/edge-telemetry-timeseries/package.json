{
    "name": "edge-telemetry-timeseries",
    "type": "commonjs",
    "version": "1.0.0",
    "description": "FarmIQ Edge Telemetry Timeseries Service - telemetry persistence and aggregation",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
        "start": "nyc ts-node src/index.ts",
        "build": "tsc && npm run copy-openapi",
        "copy-openapi": "cp openapi.yaml dist/",
        "serve": "node dist/index.js",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "lint": "eslint --ext .ts src/",
        "lint:fix": "eslint --ext .ts src/ --fix",
        "premigrate": "tsc",
        "migrate:up": "npx prisma migrate dev --name init",
        "prisma:generate": "npx prisma generate",
        "migrate:undo": "npx prisma migrate reset --force",
        "db:migrate": "npx prisma migrate deploy",
        "db:seed": "prisma db seed",
        "coverage": "npx nyc report --reporter=html && npx opener coverage/index.html",
        "generate-api-types": "openapi-typescript openapi.yaml -o src/api-types.ts"
    },
    "keywords": [],
    "author": "",
    "license": "SEE LICENSE IN LICENSE.md",
    "dependencies": {
        "@prisma/client": "^5.16.1",
        "@types/express": "^4.17.17",
        "cors": "^2.8.5",
        "dd-trace": "^4.46.0",
        "express": "^4.21.0",
        "helmet": "^7.0.0",
        "js-yaml": "^4.1.0",
        "swagger-ui-express": "^5.0.0",
        "ts-dotenv": "^0.9.1",
        "uuid": "^9.0.0",
        "winston": "^3.10.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.5",
        "@types/cors": "^2.8.13",
        "@types/jest": "^29.5.12",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^20.16.5",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^9.0.8",
        "chai": "^4.5.0",
        "dotenv-cli": "^7.2.1",
        "eslint-config-prettier": "^9.0.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "nyc": "^15.1.0",
        "opener": "^1.5.2",
        "prisma": "^5.16.1",
        "source-map-support": "^0.5.21",
        "ts-jest": "^29.1.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    },
    "prisma": {
        "seed": "npx ts-node --project tsconfig.prisma.json --transpile-only prisma/seed.ts"
    }
}

