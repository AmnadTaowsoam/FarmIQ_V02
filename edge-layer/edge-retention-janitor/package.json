    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.16.5",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.8",
